<h1>Editing A User</h1>

<%= simple_form_for @a_user,
    :url => url_for(:action => 'update', :controller => 'a_users'),
    :method => 'put' do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
    <%= f.input :role %>
    <%= f.input :user_id %>
    <%= f.input :center_name %>
    <%= f.input :course_name %>
    <%= f.input :student_name %>
    <%= f.input :dob %>
    <%= f.input :qualification %>
    <%= f.input :state_name %>
    <%= f.input :address %>
    <%= f.input :mobile_number %>
    <%= f.input :center_code %>
    <%= f.input :center_place %>
    <%= f.input :admission_date %>
    <%= f.input :gender %>
    <%= f.input :nationality %>
    <%= f.input :district_name %>
    <%= f.input :district_name %>



  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<%= link_to 'Show', a_users_path %> |
<%= link_to 'Back', a_users_path %>
